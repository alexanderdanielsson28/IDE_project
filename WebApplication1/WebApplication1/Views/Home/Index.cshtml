<!-- Navigationbar -->
<div id="fullwidth1pxheight">
    <div id="tinybox"></div>
    <!-- Displays the editor in full screen -->
    <div id="f11" class="clickdivf11f11r"><span title="Editor fullscreen">FullScreen</span></div>
    <!-- Reverts the editor into window mode -->
    <div id="f11r" class="clickdivf11f11r"><span title="Editor window">Window mode</span></div>
    <!-- Background color -->
    <div class="dropdown">
        <p id="dropit" class="dropbtn">Background</p>
        <div id="myDropdown" class="dropdown-content">
            <p id="white" class="p-aligncenter"><span title="Background white">White</span></p>
            <p id="black" class="p-aligncenter"><span title="Background dark">Dark</span></p>
            <p id="grayblue" class="p-aligncenter"><span title="Background lightgray">Lightgray</span></p>
        </div>
    </div>
    <!-- Resolution -->
    <div class="dropdown">
        <p id="res" class="dropbtn">Resolution</p>
        <div id="myDropdownRes" class="dropdown-content">
            <p id="first" class="p-aligncenter"><span title="Tiny editor">600 x 450</span></p>
            <p id="second" class="p-aligncenter"><span title="Medium editor">900 x 600</span></p>
            <p id="third" class="p-aligncenter"><span title="Large editor">1200 x 800</span></p>
        </div>
    </div>
    <!-- Select different programming languages -->
    <div class="dropdown">
        <p id="lang" class="dropbtn">Select language</p>
        <div id="myDropdownLan" class="dropdown-content">
            <p class="p-aligncenter" id="w110"><span title="Webb">HTML</span></p>
            <p class="p-aligncenter" id="w111"><span title="Styling">CSS</span></p>
            <p class="p-aligncenter" id="w112"><span title="Functions">JavaScript</span></p>
            <p class="p-aligncenter" id="w113"><span title="Object-oriented">C#</span></p>
            <p class="p-aligncenter" id="w114"><span title="Webb/functions">PHP</span></p>
            <p class="p-aligncenter" id="w115"><span title="Database">SQL</span></p>
        </div>
    </div>
    <!-- Select different editor themes -->
    <div class="dropdown">
        <p id="theme" class="dropbtn">Select theme</p>
        <div id="myDropdownTheme" class="dropdown-content">
            <p class="p-aligncenter" id="monokai"><span title="A dark theme">monokai</span></p>
            <p class="p-aligncenter" id="iplastic"><span title="A gray-white theme">iplastic</span></p>
            <p class="p-aligncenter" id="cobalt"><span title="A dark-blue theme">cobalt</span></p>
            <p class="p-aligncenter" id="tomorrow"><span title="A white theme">tomorrow</span></p>
</div>
    </div>
   

    <!-- open and close settings -->
    <div id="boxie"></div>
    <div id="boxieR"></div>
    <button type="button" class="btn">
        <span class="glyphicon glyphicon-floppy-saved"></span>Save
    </button>
    <div id="form_button">
      
        @using (Html.BeginForm())
        {

        // <input type="text" id="testData" />
        <input style="display:none" type="text" id="testData" name="reportName" />

        <input type="submit" value="Run preview" id="preview_button" />
        }
    </div>
</div>

<!-- Show navigation bar -->
<div id="twinbox">
    <div id="tinyboxtwin"></div>
</div>
<!-- Settings -->
<div id="showcontent">
    <h2 id="CommandHeader">Settings</h2>
    <div class="myboxopenarea">
        <div class="anitfloat">
            <div class="clickmetoopen" id="firstopen"></div>
            <p class="textOpener">Editor resolutions</p>
        </div>
        <div class="anitfloat" id="testfloat">
            <p class="spanCommandP"><span title="Transforms the editor into full screen size" id="full1" class="spanpointer">Fullscreen</span></p>
            <p class="spanCommandP"><span title="Revert the full screen editor into normal size" id="rev1" class="spanpointer">Window Mode</span></p>
            <p class="spanCommandP"><span title="Editor resolution w600px - h450px" id="res1" class="spanpointer">600 x 450 Resolution</span></p>
            <p class="spanCommandP"><span title="Editor resolution w900px - h600px" id="res2" class="spanpointer">900 x 600 Resolution</span></p>
            <p class="spanCommandP"><span title="Editor resolution w1200px - h800px" id="res3" class="spanpointer">1200 x 800 Resolution</span></p>
        </div>
        <div class="anitfloat">
            <div class="clickmetoopenLang" id="thirdopen"></div>
            <p class="textOpener">Editor coding languages</p>
        </div>
        <div class="anitfloat" id="imtestfloating">
            <p class="spanCommandP"><span title="Changes editor language to HTML" id="html1" class="spanpointer">HTML</span></p>
            <p class="spanCommandP"><span title="Changes editor language to CSS" id="css1" class="spanpointer">CSS</span></p>
            <p class="spanCommandP"><span title="Changes editor language to JavaScript" id="js1" class="spanpointer">JavaScript</span></p>
            <p class="spanCommandP"><span title="Changes editor language to C#" id="cs1" class="spanpointer">C#</span></p>
            <p class="spanCommandP"><span title="Changes editor language to PHP" id="phps" class="spanpointer">PHP</span></p>
            <p class="spanCommandP"><span title="Changes editor language to SQL" id="sqls" class="spanpointer">SQL</span></p>
        </div>
        <div class="anitfloat">
            <div class="clickmetoopenBK" id="fourthopen"></div>
            <p class="textOpener">Editor background</p>
        </div>
        <div class="anitfloat" id="testfloatBK">
            <p class="spanCommandP"><span title="Gives the background a dark color" id="b1" class="spanpointer">Dark background</span></p>
            <p class="spanCommandP"><span title="Gives the background a white color" id="w1" class="spanpointer">White background</span></p>
            <p class="spanCommandP"><span title="Gives the background a lightgray color" id="gb1" class="spanpointer">Lightgray background</span></p>
        </div>
        <div class="anitfloat">
            <div class="clickmetoopenThemes" id="secondopen"></div>
            <p class="textOpener">Editor themes</p>
        </div>
        <div class="anitfloat" id="testfloatThemes">
            <p class="spanCommandP"><span title="Changes editor theme to monokai(dark)" id="mn1" class="spanpointer">monokai theme</span></p>
            <p class="spanCommandP"><span title="Changes editor theme to iplastic(bright)" id="isp1" class="spanpointer">iplastic theme</span></p>
            <p class="spanCommandP"><span title="Changes editor theme to cobalt(dark-blue)" id="cb1" class="spanpointer">cobalt theme</span></p>
            <p class="spanCommandP"><span title="Changes editor theme to tomorrow(white)" id="tm1" class="spanpointer">tomorrow theme</span></p>
        </div>
    </div>
</div>
